# Environment Variables for PoEHR Mobile
# Copy this file to .env.local and update values for your environment

# API Configuration
EXPO_PUBLIC_API_BASE_URL=https://your-production-domain.com
EXPO_PUBLIC_API_TIMEOUT=10000
EXPO_PUBLIC_APP_ENV=production

# App Configuration
EXPO_PUBLIC_APP_VERSION=1.0.0
EXPO_PUBLIC_APP_NAME=PoEHR Mobile

# Debug Settings (set to false for production)
EXPO_PUBLIC_DEBUG_API=false
EXPO_PUBLIC_ENABLE_LOGGING=false

# Optional: Analytics and Monitoring
# EXPO_PUBLIC_SENTRY_DSN=your-sentry-dsn
# EXPO_PUBLIC_ANALYTICS_ID=your-analytics-id
